{
  "cSpell.language": "en,es",
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": "always",
    "source.organizeImports": "always"
  },
  "editor.defaultFormatter": "biomejs.biome",
  "cSpell.words": [
    "adminer",
    "Instantiator",
    "loadbalancer",
    "nestjs",
    "nginx",
    "postgis",
    "postgres",
    "Uncapitalize"
  ]
}
